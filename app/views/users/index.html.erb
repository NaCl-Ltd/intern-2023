<% provide(:title, 'All users') %>
<h1>全てのユーザー</h1>

<%= will_paginate %>
<%= form_with url:search_path, method: :post do |form| %>
  <%= form.label :query, "ユーザ検索:" %>
  <%= form.text_field :name %>
  <%= form.submit "検索" %>
<% end %>
<ul class="users">
    <%= render @users%>
</ul>

<%= will_paginate %>
